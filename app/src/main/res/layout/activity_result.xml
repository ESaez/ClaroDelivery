<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/activity_result"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.autentia.clarodelivery.ResultActivity">

    <android.support.v7.widget.Toolbar
        android:id="@id/toolbar"
        android:layout_width="match_parent"
        android:layout_height="?attr/actionBarSize"
        android:background="@color/colorPrimary_claro"
        app:title="@string/resumen_negocio"
        app:titleTextAppearance="@style/Toolbar.TitleText"
        app:titleTextColor="@android:color/white" />

    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_marginTop="?attr/actionBarSize">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_marginBottom="10dp"
            android:layout_marginLeft="16dp"
            android:layout_marginRight="16dp"
            android:orientation="vertical">

            <!--<RelativeLayout-->
            <!--android:layout_width="match_parent"-->
            <!--android:layout_height="match_parent"-->
            <!--android:layout_marginTop="?attr/actionBarSize"-->
            <!--android:layout_marginBottom="10dp"-->
            <!--android:layout_marginLeft="16dp"-->
            <!--android:layout_marginRight="16dp">-->

            <android.support.design.widget.TextInputLayout
                android:id="@+id/input0"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_below="@id/imageView"
                android:layout_centerHorizontal="true"
                android:ems="10"
                android:inputType="number">

                <EditText
                    android:id="@+id/et_nombre_vendedor"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:focusable="false"
                    android:focusableInTouchMode="false"
                    android:hint="Nombre vendedor"
                    android:maxLength="50"
                    android:maxLines="1"
                    android:minLines="1"
                    android:textAppearance="?android:textAppearanceSmall"
                    android:textColor="@color/colorPrimary_claro"
                    android:textColorHint="@color/colorPrimaryDark_claro"
                    android:textSize="12sp" />

            </android.support.design.widget.TextInputLayout>

            <android.support.design.widget.TextInputLayout
                android:id="@+id/input9"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_below="@id/input0"
                android:layout_centerHorizontal="true"
                android:layout_marginTop="5dp"
                android:ems="10"
                android:inputType="number">

                <EditText
                    android:id="@+id/et_rut_vendedor"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:focusable="false"
                    android:focusableInTouchMode="false"
                    android:hint="Rut vendedor"
                    android:maxLength="50"
                    android:maxLines="1"
                    android:minLines="1"
                    android:textAppearance="?android:textAppearanceSmall"
                    android:textColor="@color/colorPrimary_claro"
                    android:textColorHint="@color/colorPrimaryDark_claro"
                    android:textSize="12sp" />

            </android.support.design.widget.TextInputLayout>

            <android.support.design.widget.TextInputLayout
                android:id="@+id/input1"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_below="@id/input9"
                android:layout_centerHorizontal="true"
                android:layout_marginTop="5dp"
                android:ems="10"
                android:inputType="number">

                <EditText
                    android:id="@+id/et_nombre_completo"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:focusable="false"
                    android:focusableInTouchMode="false"
                    android:hint="Nombre cliente"
                    android:maxLength="50"
                    android:maxLines="1"
                    android:minLines="1"
                    android:textAppearance="?android:textAppearanceSmall"
                    android:textColor="@color/colorPrimary_claro"
                    android:textColorHint="@color/colorPrimaryDark_claro"
                    android:textSize="12sp" />

            </android.support.design.widget.TextInputLayout>

            <android.support.design.widget.TextInputLayout
                android:id="@+id/input6"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_below="@id/input1"
                android:layout_centerHorizontal="true"
                android:layout_marginTop="5dp"
                android:ems="10"
                android:inputType="number">

                <EditText
                    android:id="@+id/et_rut_usuario"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:focusable="false"
                    android:focusableInTouchMode="false"
                    android:hint="Rut cliente"
                    android:maxLength="10"
                    android:maxLines="1"
                    android:minLines="1"
                    android:textAppearance="?android:textAppearanceSmall"
                    android:textColor="@color/colorPrimary_claro"
                    android:textColorHint="@color/colorPrimaryDark_claro"
                    android:textSize="12sp" />

            </android.support.design.widget.TextInputLayout>

            <android.support.design.widget.TextInputLayout
                android:id="@+id/input2"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_below="@+id/input6"
                android:layout_centerHorizontal="true"
                android:layout_centerVertical="true"
                android:layout_marginTop="5dp">

                <EditText
                    android:id="@+id/et_email_resultado"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:focusable="false"
                    android:focusableInTouchMode="false"
                    android:hint="E-mail"
                    android:inputType="textEmailAddress"
                    android:maxLength="30"
                    android:maxLines="1"
                    android:minLines="1"
                    android:textAppearance="?android:textAppearanceSmall"
                    android:textColor="@color/colorPrimary_claro"
                    android:textColorHint="@color/colorPrimaryDark_claro"
                    android:textSize="12sp" />

            </android.support.design.widget.TextInputLayout>

            <android.support.design.widget.TextInputLayout
                android:id="@+id/input3"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_below="@+id/input2"
                android:layout_centerHorizontal="true"
                android:layout_centerVertical="true"
                android:layout_marginTop="5dp">

                <EditText
                    android:id="@+id/et_numero_servicio"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:focusable="false"
                    android:focusableInTouchMode="false"
                    android:hint="N° de solicitud de servicio"
                    android:inputType="number"
                    android:maxLength="50"
                    android:maxLines="1"
                    android:minLines="1"
                    android:textAppearance="?android:textAppearanceSmall"
                    android:textColor="@color/colorPrimary_claro"
                    android:textColorHint="@color/colorPrimaryDark_claro"
                    android:textSize="12sp" />

            </android.support.design.widget.TextInputLayout>

            <android.support.design.widget.TextInputLayout
                android:id="@+id/input4"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_below="@+id/input3"
                android:layout_centerHorizontal="true"
                android:layout_centerVertical="true"
                android:layout_marginTop="5dp">

                <EditText
                    android:id="@+id/et_numero_contrato"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:focusable="false"
                    android:focusableInTouchMode="false"
                    android:hint="N° de contrato de arriendo"
                    android:inputType="number"
                    android:maxLength="15"
                    android:maxLines="1"
                    android:minLines="1"
                    android:textAppearance="?android:textAppearanceSmall"
                    android:textColor="@color/colorPrimary_claro"
                    android:textColorHint="@color/colorPrimaryDark_claro"
                    android:textSize="12sp" />

            </android.support.design.widget.TextInputLayout>

            <android.support.design.widget.TextInputLayout
                android:id="@+id/input5"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_below="@+id/input4"
                android:layout_centerHorizontal="true"
                android:layout_centerVertical="true"
                android:layout_marginTop="5dp">

                <EditText
                    android:id="@+id/et_codigo_auditoria"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:focusable="false"
                    android:focusableInTouchMode="false"
                    android:hint="Código auditoria"
                    android:inputType="number"
                    android:maxLength="30"
                    android:maxLines="1"
                    android:minLines="1"
                    android:textAppearance="?android:textAppearanceSmall"
                    android:textColor="@color/colorPrimary_claro"
                    android:textColorHint="@color/colorPrimaryDark_claro"
                    android:textSize="12sp" />

            </android.support.design.widget.TextInputLayout>

            <android.support.design.widget.TextInputLayout
                android:id="@+id/input7"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_below="@+id/input5"
                android:layout_centerHorizontal="true"
                android:layout_centerVertical="true"
                android:layout_marginTop="5dp">

                <EditText
                    android:id="@+id/et_status"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:focusable="false"
                    android:focusableInTouchMode="false"
                    android:hint="Fecha / Operador - Tipo - Nivel"
                    android:inputType="number"
                    android:maxLength="50"
                    android:maxLines="1"
                    android:minLines="1"
                    android:textColor="@color/colorPrimary_claro"
                    android:textColorHint="@color/colorPrimaryDark_claro"
                    android:textSize="12sp" />

            </android.support.design.widget.TextInputLayout>


            <Button
                android:id="@+id/bt_guardar_evidencia"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_below="@+id/input7"
                android:layout_centerHorizontal="true"
                android:layout_marginTop="5dp"
                android:background="@android:color/darker_gray"
                android:paddingLeft="20dp"
                android:text="Terminar Verificación" />


        </LinearLayout>

    </ScrollView>

</RelativeLayout>
